
<div class="container">
  <div class="row">
   <div class="col-lg-12">
   <h1 class="page-header">People</h1>
   </div>
    <% if user_signed_in? %>
      <% @user.each do |user| %>
        <% unless user.id == current_user.id %>
          <div class="col-lg-3 col-md-4 col-xs-6 thumb">
            <a class="thumbnail" href="#">
            <%= image_tag user.image.url %>
            </a>
          </div>
        <% end %>
      <% end %>
    <% end %>
  </div>
</div>

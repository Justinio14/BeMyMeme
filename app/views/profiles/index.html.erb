<html>

<% if user_signed_in? %>
  <% @user.each do |user| %>
    <% unless user.id == current_user.id %>
      <div class="user">
        <%= image_tag user.image.url(:thumb) %><br />
        <%= user.username %><br />
        <%= user.gender %><br />
        <%= user.location %><br/>
          <br />
        </div>
    </br/>
    <% end %>
  <% end %>
<% end %>

</html>

<div class="container" style="background-color: #F9F3EB; width:inherit;">
<h3 id="myModalLabel" style="text-align:center; text-decoration: underline;">Meme Gallery</h3> 
<%@memes.each do |meme| %> 
<div>
<%=image_tag meme.name, :style=>"height: 300px; height: 300px; padding: 20px;"%>
<form action='/add' method='post' target='dummyframe'> 
  <input  type='text' style='display:none' name="id" value="<%=meme.id%>"> 
  <input type='submit' value='Save'> 
</form>
</div>
  <br> 
<%end%>
</div>

<div class="row">
  <div class="col-md-9">
    <ul id="meme-convos-list">
      <% @meme_convos.each do |convo| %>
        <%= render 'meme_convos/meme_convo', meme_convo: meme_convo, user: current_user %>
      <% end %>
    </ul>
  </div>
 
  <div class="col-md-3">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">User list</h3>
      </div>
      <div class="panel-body">
        <ul>
          <% @users.each do |user| %>
            <li><%= link_to user.username, meme_convos_path(user_id: user), remote: true, method: :post %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>